   "a": {
      "category": "Basic",
      "hiragana": "\u3042",
      "hiragana_note": "Ah! You found the letter A. Look for the letter A.",
      "katakana": "\u30a2",
      "katakana_note": "Ark. Think of Noah\u2019s Ark.",
      "romaji": "a"
   },
   "i": {
      "category": "Basic",
      "hiragana": "\u3044",
      "hiragana_note": "I\u2019ve never been to Hawaii. Think of two palm trees. These are the ii\u2019s in Hawaii.",
      "katakana": "\u30a4",
      "katakana_note": "Evil eagle. Think of an evil eagle perched on a capital \u2019T\u2019. This symbol looks like a \u2019T\u2019.",
      "romaji": "i"
   },
   "u": {
      "category": "Basic",
      "hiragana": "\u3046",
      "hiragana_note": "The dude with the food. He eats and makes an \u2018oo\u2019 sound.",
      "katakana": "\u30a6",
      "katakana_note": "UFU RAWA. She is an Unlucky Unicorn. Here she has her fur and horn.",
      "romaji": "u"
   },
   "e": {
      "category": "Basic",
      "hiragana": "\u3048",
      "hiragana_note": "Ed the pelican. He makes an \u2018eh\u2019 sound.",
      "katakana": "\u30a8",
      "katakana_note": "Egg Timer. An egg timer keeps the time eggsactly.",
      "romaji": "e"
   },
   "o": {
      "category": "Basic",
      "hiragana": "\u304a",
      "hiragana_note": "Oh! Throw a dog a bone. Think of all the \u2018o\u2019 sounds.",
      "katakana": "\u30aa",
      "katakana_note": "Old Man. Think of his long beard. The vertical line is his leg but his ankle is broken. OH! It hurts.",
      "romaji": "o"
   },
   "ka": {
      "category": "Basic",
      "hiragana": "\u304b",
      "hiragana_note": "Batman will never Katch the Jo-KA! Think of Batman punching the Joker\u2019s face. KA-POW!",
      "katakana": "\u30ab",
      "katakana_note": "Batman will never Katch the Jo-KA! Think of the Joker\u2019s face.",
      "romaji": "ka"
   },
   "ki": {
      "category": "Basic",
      "hiragana": "\u304d",
      "hiragana_note": "The key is to keep thinking. This symbol looks like a key.",
      "katakana": "\u30ad",
      "katakana_note": "The key is to keep thinking. This looks like a key.",
      "romaji": "ki"
   },
   "ku": {
      "category": "Basic",
      "hiragana": "\u304f",
      "hiragana_note": "Ku ku! Ku ku! Think of a cuckoo bird.",
      "katakana": "\u30af",
      "katakana_note": "Kuketa\u2019s Kumfy Kushion. Meet Kuketa. Here he is sleeping on his kumfy kushion.",
      "romaji": "ku"
   },
   "ke": {
      "category": "Basic",
      "hiragana": "\u3051",
      "hiragana_note": "Kemistry set. Think of a skinny test tube and a water dropper from a chemistry set.",
      "katakana": "\u30b1",
      "katakana_note": "Some kerfuffle is keeping hime awake. Now there is something on the cushion keeping him awake. Perhaps it\u2019s a tag\u2026",
      "romaji": "ke"
   },
   "ko": {
      "category": "Basic",
      "hiragana": "\u3053",
      "hiragana_note": "It\u2019s a Ko-py. The bottom line is a copy of the top.",
      "katakana": "\u30b3",
      "katakana_note": "A KOrner is missing. This symbol is linked to the RO which depicts a solid rock. But here it is missing a corner.",
      "romaji": "ko"
   },
   "sa": {
      "category": "Basic",
      "hiragana": "\u3055",
      "hiragana_note": "Sam is always right. Think of a face that looks to the right.",
      "katakana": "\u30b5",
      "katakana_note": "Saddle. Think of a huge saddle for a sad elephant who is drunk of sake.",
      "romaji": "sa"
   },
   "shi": {
      "category": "Basic",
      "hiragana": "\u3057",
      "hiragana_note": "Shi has shiny hair. Think of a girl with long hair.",
      "katakana": "\u30b7",
      "katakana_note": "Shi-tsu dogs. Shi is the girl and looks to the right. Tsu is the boy and looks to the left.",
      "romaji": "shi"
   },
   "su": {
      "category": "Basic",
      "hiragana": "\u3059",
      "hiragana_note": "Super Sumo! Imagine a caped superman sumo swooping into action.",
      "katakana": "\u30b9",
      "katakana_note": "Sue the nurse is late for surgery. Think of Sue running at full speed through a hospital.",
      "romaji": "su"
   },
   "se": {
      "category": "Basic",
      "hiragana": "\u305b",
      "hiragana_note": "I seriously hate syringes. Think of a seriously big syringe injecting an arm.",
      "katakana": "\u30bb",
      "katakana_note": "Serenading. Think of a man serenading a cute girl. It\u2019s certainly sensual.",
      "romaji": "se"
   },
   "so": {
      "category": "Basic",
      "hiragana": "\u305d",
      "hiragana_note": "The Penguin is SO fat. Think of his pointy nose, mustache and fat chin.",
      "katakana": "\u30bd",
      "katakana_note": "So\u2026want to come back to my kennel? Tsu winks at Shi so one of his eyes is hidden.",
      "romaji": "so"
   },
   "ta": {
      "category": "Basic",
      "hiragana": "\u305f",
      "hiragana_note": "Ta. This is a simple one. It looks like a ta.",
      "katakana": "\u30bf",
      "katakana_note": "Take that you tag! Now he angrily tears off the itchy tag and folds it under his cushion.",
      "romaji": "ta"
   },
   "chi": {
      "category": "Basic",
      "hiragana": "\u3061",
      "hiragana_note": "Five chirping chicks. Look for the number 5.",
      "katakana": "\u30c1",
      "katakana_note": "Nacho the Chili Pepper. Think of a long chinned cheerful chili pepper with a sombrero.",
      "romaji": "chi"
   },
   "tsu": {
      "category": "Basic",
      "hiragana": "\u3064",
      "hiragana_note": "Tsunami! Imagine a giant tsunami wave.",
      "katakana": "\u30c4",
      "katakana_note": "Tsu promises Shi a tsunami of kisses. Now he has both eyes wide open in anticipation.",
      "romaji": "tsu"
   },
   "te": {
      "category": "Basic",
      "hiragana": "\u3066",
      "hiragana_note": "Ten. This symbol looks like the letter \u2019t\u2019.",
      "katakana": "\u30c6",
      "katakana_note": "Teddy Bear\u2019s Teapot. Think of a cute little teapot at a teddy bear\u2019s picnic.",
      "romaji": "te"
   },
   "to": {
      "category": "Basic",
      "hiragana": "\u3068",
      "hiragana_note": "Thorn in my toe. Imagine a thorn stuck in a toe.",
      "katakana": "\u30c8",
      "katakana_note": "Tick TOck tick TOck. Think of a totally awesome wrist watch ticking.",
      "romaji": "to"
   },
   "na": {
      "category": "Basic",
      "hiragana": "\u306a",
      "hiragana_note": "Throw the narwhal a banana. Think of a narwhal fish eating a banana.",
      "katakana": "\u30ca",
      "katakana_note": "Natural and Naked Chili. Now Nacho the Chili is missing his sombrero.",
      "romaji": "na"
   },
   "ni": {
      "category": "Basic",
      "hiragana": "\u306b",
      "hiragana_note": "Kneeling ninja. Think of a ninja kneeling with his sword.",
      "katakana": "\u30cb",
      "katakana_note": "Ni is the Japanese word for \u201ctwo\u201d and this symbol has two neat lines.",
      "romaji": "ni"
   },
   "nu": {
      "category": "Basic",
      "hiragana": "\u306c",
      "hiragana_note": "New Noodles. It\u2019s a curly tailed noodle held by chopsticks.",
      "katakana": "\u30cc",
      "katakana_note": "Sue the NUrse slips on a NOOdle and needs a NEW bag. She breaks her leg and now it pokes through her waste.",
